{
  "name": "unity-ci-teamcity-bridge",
  "version": "0.0.1",
  "description": "Processes webhooks from Unity Cloud builds and triggers teamcity builds",
  "author": "Dubit",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "test": "jest --config config/jest.config.json",
    "build": "webpack",
    "watch": "concurrently --kill-others 'npm run build-and-watch' 'npm run serve-and-watch'",
    "build-and-watch": "webpack --watch",
    "serve-and-watch": "nodemon --watch dist dist/server.js"
  },
  "dependencies": {
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.2.0",
    "koa-request": "^1.0.0",
    "koa-router": "^5.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.2.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.14.0",
    "jest": "^22.4.2",
    "nodemon": "^1.10.2",
    "webpack": "^1.13.2"
  }
}
